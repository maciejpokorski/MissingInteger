{
    "env": {
        "jest": true
    },
    "rules": {
        "linebreak-style": "off"
    },
    "plugins": [
        "flowtype"
    ],
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module",
      "allowImportExportEverywhere": true,
      "codeFrame": true
    }
}